{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "likes": {
      "type": "biginteger",
      "default": "0"
    },
    "titleImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "newspapers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::newspaper.newspaper"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Прогнозы",
        "Кейсы",
        "Интервью"
      ],
      "required": true
    }
  }
}
